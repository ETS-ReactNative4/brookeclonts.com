{
  "name": "brookeclonts",
  "version": "1.0.0",
  "description": "This is Brooke Clonts' Author Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:prod && NODE_ENV=production node ./build/server/index.js",
    "build": "babel ./src -d build",
    "build:watch": "babel ./src -d build --watch",
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "npm run build && npm run build:client",
    "start:dev": "parallelshell 'npm run build:watch' 'npm run build:watch:client' 'nodemon ./build/server/index.js'",
    "test": "jest --watch --coverage",
    "update-snapshots": "jest --updateSnapshot",
    "eject": "react-scripts eject",
    "webpack": "webpack -w"
  },
  "presets": [
    "es2015"
  ],
  "proxy": "http://localhost:3001",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brookeclonts/brookeclonts.com.git"
  },
  "keywords": [
    "Brooke",
    "Clonts",
    "Author",
    "Salt",
    "Lake",
    "City",
    "Utah"
  ],
  "author": "Brooke Clonts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brookeclonts/brookeclonts.com/issues"
  },
  "homepage": "https://github.com/brookeclonts/brookeclonts.com#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "emotion": "^8.0.12",
    "express": "^4.14.0",
    "express-react-router": "^3.0.2",
    "mongodb": "^2.2.24",
    "mongoose": "^4.8.4",
    "morgan": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.0.6",
    "jest-enzyme": "^4.0.2",
    "nodemon": "^1.14.1",
    "parallelshell": "^3.0.2",
    "react-test-renderer": "^16.2.0",
    "svg-react-loader": "^0.4.5",
    "uglifyjs": "^2.4.11",
    "webpack": "^3.10.0"
  }
}
